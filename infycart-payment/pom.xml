		</dependency>
	</dependencies>
	<build>
		<finalName>infycart-payment</finalName>
		<plugins>
			<plugin>
				<groupId>org.apache.tomcat.maven</groupId>
				<artifactId>tomcat7-maven-plugin</artifactId>
				<version>2.2</version>
				<configuration>
					<port>9092</port>
					<path>/</path>
				</configuration>
			</plugin>
		</plugins>
	</build>
  
</project>
